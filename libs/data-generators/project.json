{
  "name": "data-generators",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/data-generators/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/data-generators/**/*.ts"]
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "ts-node countryGenerators/countryGenerator.ts",
          "ts-node languageGenerators/languageGenerator.ts",
          "ts-node nameGenerators/nameGenerator.ts"
        ],
        "cwd": "libs/data-generators/src/lib"
      }
    }
  },
  "tags": []
}
